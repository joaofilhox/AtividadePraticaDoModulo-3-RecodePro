
class Cliente {
    - idCliente: int
    - nomeCliente: String
    - cpfCliente: String
    - enderecoCliente: String
    - telefoneCliente: String
    - emailCliente: String
    + Cliente()
    + Cliente(int, String, String, String, String, String)
    + getIdCliente(): int
    + getNomeCliente(): String
    + getCpfCliente(): String
    + getEnderecoCliente(): String
    + getTelefoneCliente(): String
    + getEmailCliente(): String
}

class Pacotes_de_Interesse {
    - idPacoteDeViagens: int
    - destinos: String
    - precos: String
    - idCliente: int
    + Pacotes_de_Interesse()
    + Pacotes_de_Interesse(int, String, String, String)
    + getIdPacoteDeViagens(): int
    + getDestinos(): String
    + getPrecos(): String
    + getIdCliente(): int
}

class Pagamentos {
    - idPagamento: int
    - valorPago: String
    - destinoPago: String
    - idCliente: int
    + Pagamentos()
    + Pagamentos(int, String, String, String)
    + getIdPagamento(): int
    + getValorPago(): String
    + getDestinoPago(): String
    + getIdCliente(): int
}



